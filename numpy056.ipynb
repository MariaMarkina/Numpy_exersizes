   "source": [
    "import numpy as np\n",
    "k = np.random.random((10,2)) \n",
    "x, y = k[:,0], k[:,1]\n",
    "\n",
    "des = np.sqrt(x*x+y*y)\n",
    "sigma = 1\n",
    "mu = 0\n",
    "gaus = np.exp(-1*((des-mu)**2 / (2 * sigma**2)))\n",
    "\n",
    "print(gaus)"
   ]
